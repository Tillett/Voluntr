<head>
<style>
input {
    width: 10%;
}
</style>
</head>
<h3>Availability</h3>
<section class = container-section>
<form>
<%= form_for(@availability) do |f| %>
  <%= f.label :mon_st %>
  <%= f.time_field :mon_st %>
  
  <%= f.label :mon_en %>
  <%= f.time_field :mon_en %>
  <p><br /></p>
  <%= f.label :tues_st %>
  <%= f.time_field :tues_st %>
  
  <%= f.label :tues_en %>
  <%= f.time_field :tues_en %>
  <p><br /></p>
  <%= f.label :wed_st %>
  <%= f.time_field :wed_st %>
  
  <%= f.label :wed_en %>
  <%= f.time_field :wed_en %>
  <p><br /></p>
  <%= f.label :thur_st %>
  <%= f.time_field :thur_st %>
  
  <%= f.label :thur_en %>
  <%= f.time_field :thur_en %>
  <p><br /></p>
  <%= f.label :fri_st %>
  <%= f.time_field :fri_st %>
  
  <%= f.label :fri_en %>
  <%= f.time_field :fri_en %>
  <p><br /></p>
  <%= f.label :sat_st %>
  <%= f.time_field :sat_st %>
  
  <%= f.label :sat_en %>
  <%= f.time_field :sat_en %>
  <p><br /></p>
  <%= f.label :sun_st %>
  <%= f.time_field :sun_st %>
  
  <%= f.label :sun_en %>
  <%= f.time_field :sun_en %>
  <p><br /></p>
  <%= f.submit "Update" %>
  
<% end %>
</form>
</section>