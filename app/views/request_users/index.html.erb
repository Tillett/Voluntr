<% provide(:title, 'Seach users') %>
<h1>Search for an Organization</h1>

<%= form_tag requsers_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>

<ul class="request_users">
  <% @request_users.each do |user| %>
    <li>
      <%= link_to user.display_name, user %>
    </li>
  <% end %>
</ul>