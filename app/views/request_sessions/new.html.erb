<h1>Organization Login</h1>
<p1><%= link_to "Don't have an account? Register here", reqsignup_path %></p1>
<p>
  <%= form_for(:session, url: reqlogin_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    
    <%= f.label :remember_me do %>
      <%= f.check_box :remember_me %>
      <span>Remember me on this computer</span>
    <% end %>
    
    <%= f.submit "Log in" %>
  <% end %>
</p>
