<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <nav>
      <%= link_to image_tag("Voluntr2.png", border:0), home_url %>
        <ul class="nav navbar-nav navbar-right">
          <li>
            <%= link_to "Home", home_url  %>
          </li>
          <li>
            <%= link_to "About", about_path %>
          </li>
          <% if volunteer_user_logged_in? %>
            <%= render 'layouts/_vollinks' %>
              <% end %>
                <% if request_user_logged_in? %>
                  <%= render 'layouts/_reqlinks' %>
                    <% end  %>
                      <% if !volunteer_user_logged_in? && !request_user_logged_in?  %>
                        <%= render 'layouts/_login_drop' %>
                          <%= render 'layouts/_register_drop' %>
                            <% end %>
                              </li>
                              <li>
                                <%= link_to "FAQ", faq_path %>
                              </li>
        </ul>
    </nav>
  </div>
</header>