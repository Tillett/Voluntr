   <h3>Post a Job!</h3>
<section class= "container-section">
<%= form_for(@request_post_job) do |f| %>
<%= f.label :title %>
<%= f.text_field :title %><br>

<%= f.label :description %>
<%= f.text_field :description %><br>

<%= f.label :avail %>
<%= f.time_field :avail %><br>

<%= hidden_field_tag(:id, params[:id]) %>
<%= f.submit "Post" %>
<% end %>
</section>
